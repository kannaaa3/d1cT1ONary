<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="946.0" style="-fx-background-color: #2C2F48;"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.GameController">
    <children>
        <Label layoutX="23.0" layoutY="25.0" prefHeight="42.0" prefWidth="170.0" text="Game Center"
               textFill="WHITE">
            <font>
                <Font name="Century Gothic" size="23.0"/>
            </font>
        </Label>
        <Button fx:id="agilityGame" layoutX="50.0" layoutY="105.0" minHeight="-Infinity"
                minWidth="-Infinity" mnemonicParsing="false" prefHeight="180.0" prefWidth="670.0"
                style="-fx-background-radius: 20;">
            <graphic>
                <ImageView fitHeight="183.0" fitWidth="672.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../assets/Game/Elevate%20Agility/Banner.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
